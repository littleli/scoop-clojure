                "https://github.com/borkdude/deps.clj/releases/download/v1.11.4.1474/deps.clj-1.11.4.1474-windows-amd64.zip",
                "https://github.com/clojure/brew-install/releases/download/1.11.4.1474/clojure-tools.zip"
