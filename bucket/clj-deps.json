            "hash": [
                "c4634ab46f72569d8a84014aed2facc683044ac73435256a8b5dc00f306b6ae4",
                "de8b0d00be9e0ffe96e34a0a6503228dccbda6b83d09255e65190a83f11823ea"
            ]
        }
    },
    "pre_install": [
        "Move-Item \"$dir\\ClojureTools\\*\" \"$dir\\\"",
