{
    "version": "0.1.10",
    "description": "Babashka to serve static assets",
    "homepage": "https://github.com/babashka/http-server",
    "license": "MIT",
    "depends": "scoop-clojure/babashka",
    "architecture": {
        "64bit": {
            "url": "https://github.com/babashka/http-server/releases/download/v0.1.10/http-server.jar",
            "hash": "255b68e7ddeb23222c94f915b8e38e59b38f24fa7c188f42e381ca9cdf933784"
        }
    },
    "pre_install": "Set-Content \"$dir\\http-server.bat\" \"@bb.exe \"\"%~dp0http-server.jar\"\" %*\"",
    "bin": "http-server.bat",
    "checkver": {
        "url": "https://api.github.com/repos/babashka/http-server/tags",
        "regex": "\"v([\\d.]+)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/babashka/http-server/releases/download/v$version/http-server.jar"
            }
        }
    }
}
