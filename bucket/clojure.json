{
    "version": "1.10.3.839",
    "description": "Clojure is a modern, dynamic, and functional dialect of the Lisp programming language on the Java platform",
    "homepage": "https://clojure.org",
    "license": "EPL-1.0",
    "suggest": {
        "JDK": [
            "java/adopt8-hotspot",
            "java/adoptopenjdk-lts-hotspot",
            "java/oraclejdk",
            "java/openjdk"
        ]
    },
    "url": "https://download.clojure.org/install/clojure-tools-1.10.3.839.zip",
    "hash": "12f676d685d32425a9328c6b2c2a6125950548663875c9444af0f3f800d38177",
    "extract_dir": "ClojureTools",
    "psmodule": {
        "name": "ClojureTools"
    },
    "bin": [
        [
            "powershell.exe",
            "cmd-clojure",
            "-NoProfile -ExecutionPolicy Bypass -Command Invoke-Clojure"
        ],
        [
            "powershell.exe",
            "cmd-clj",
            "-NoProfile -ExecutionPolicy Bypass -Command Invoke-Clojure"
        ]
    ],
    "checkver": {
        "url": "https://clojure.org/releases/tools",
        "re": "(\\d+\\.\\d+\\.\\d+\\.\\d+) \\("
    },
    "autoupdate": {
        "url": "https://download.clojure.org/install/clojure-tools-$version.zip"
    }
}
