{
    "version": "1.12.0.1501",
    "description": "Use `clj-deps`. Clojure is a modern, dynamic, and functional dialect of the Lisp programming language on the Java platform",
    "homepage": "https://clojure.org",
    "license": "EPL-1.0",
    "notes": [
        "Please fully exit and restart any active terminal sessions.",
        "-----------------------------------------------------------",
        "IMPORTANT NOTICE: This is no longer prefered way to install Clojure on Windows.",
        "To migrate execute following:",
        "   scoop uninstall clojure",
        "   scoop install clj-deps"
    ],
    "suggest": {
        "JDK": [
            "java/openjdk",
            "java/temurin-jdk",
            "java/oraclejdk"
        ]
    },
    "url": "https://github.com/clojure/brew-install/releases/download/1.12.0.1501/clojure-tools.zip",
    "hash": "447fffcddb9bd55c58c3eb59983603ac71dd38e65b73fc5c6a644b3c1bcfbe63",
    "extract_dir": "ClojureTools",
    "psmodule": {
        "name": "ClojureTools"
    },
    "bin": [
        [
            "powershell.exe",
            "cmd-clojure",
            "-NoProfile -ExecutionPolicy Bypass -Command Invoke-Clojure"
        ],
        [
            "powershell.exe",
            "cmd-clj",
            "-NoProfile -ExecutionPolicy Bypass -Command Invoke-Clojure"
        ]
    ],
    "checkver": {
        "github": "https://github.com/clojure/brew-install"
    },
    "autoupdate": {
        "url": "https://github.com/clojure/brew-install/releases/download/$version/clojure-tools.zip"
    }
}
