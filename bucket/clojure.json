    "bin": [
        [
            "powershell.exe",
            "cmd-clojure",
            "-NoProfile -ExecutionPolicy Bypass -Command Invoke-Clojure"
        ],
        [
            "powershell.exe",
            "cmd-clj",
            "-NoProfile -ExecutionPolicy Bypass -Command Invoke-Clojure"
        ]
    ],
    "checkver": {
        "github": "https://github.com/clojure/brew-install"
